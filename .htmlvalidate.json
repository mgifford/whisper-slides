{
  "extends": ["html-validate:recommended"],
  "rules": {
    "no-raw-characters": "error",
    "unique-landmark": "error",
    "hidden-focusable": "error",
    "no-inline-style": "warn",
    "no-trailing-whitespace": "off",
    "void-style": "off",
    "element-permitted-content": "warn",
    "require-sri": "off"
  }
}
